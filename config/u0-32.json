{
  "segment_widths": [1, 4],
  "split_whitespace": true,
  "tag_create": {"regex_string": ":(.*)"},
  "tag_use_rules": [
    {
      "regex_string": "\\.(.*)",
      "feedbacks": [
        {
          "add_segment": 1,
          "relative": false,
          "pos_segment": 0,
          "pos_offset": 0
        }
      ]
    },
    {
      "regex_string": "\\$(.*)",
      "feedbacks": [
        {
          "add_segment": 1,
          "relative": false,
          "pos_segment": 1,
          "pos_offset": 0
        }
      ]
    }
  ],
  "rules": [
    {
      "regex_string": "rread([0-3])",
      "segment_values": [[0],[]],
      "captures": [
        {
          "base": 10,
          "feedbacks": [
            {
              "shift": 0,
              "segment": 0,
              "index": 0,
              "fill": false,
              "fill_offset": 0
            }
          ]
        }
      ]
    }
  ]
}
