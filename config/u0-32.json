{
  "segment_widths": [8, 32],
  "tag_create": {"regex_string": ":(.*)"},
  "tag_use": {
    "regex_string": "\\.(.*)",
    "feedbacks": []
  },
  "rules": [
    {
      "regex_string": "rread([0-3])",
      "segment_values": [[0],[]],
      "captures": [
        {
          "base": 10,
          "feedbacks": [
            {
              "shift": 0,
              "segment": 0,
              "index": 0
            }
          ]
        }
      ]
    }
  ]
}
